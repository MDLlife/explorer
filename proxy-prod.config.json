{
  "/api/coinSupply": {
    "target": "http://explorer.mdl.life",
    "pathRewrite": {
      "^/api/coinSupply" : "explorer/getEffectiveOutputs"
    },
    "secure": false,
    "logLevel": "debug"
  },
  "/api/outputs": {
    "target": "http://explorer.mdl.life",
    "pathRewrite": {
      "^/api/outputs" : "currentBalance"
    },
    "secure": false,
    "logLevel": "debug"
  },
  "/api/address": {
    "target": "http://explorer.mdl.life",
    "pathRewrite": {
      "^/api/address" : "explorer/address"
    },
    "secure": false,
    "logLevel": "debug"
  },
  "/api/*": {
    "target": "http://explorer.mdl.life",
    "pathRewrite": {
      "^/api" : ""
    },
    "secure": false,
    "logLevel": "debug"
  }
}
